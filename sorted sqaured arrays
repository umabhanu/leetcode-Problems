program to return sorted squared values of array

import java.util.*;

class Program {

  public int[] sortedSquaredArray(int[] array) {
		int len=array.length;
		int[] arr=array;
    int[] sortedarray= new int[len];
		
		int[] c=new int[len];
		for(int i=0;i<array.length;i++)
		{
			c[i]=array[i]*array[i];
		}
    Arrays.sort(c);
		return c;
  }
}
